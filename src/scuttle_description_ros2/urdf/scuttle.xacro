<gazebo reference="l_wheel">
  <material>Gazebo/Blue</material>
  <mu1>1.0</mu1>
  <mu2>1.0</mu2>
  <minDepth>0.005</minDepth>
  <kp>1000000.0</kp>
  <kd>100.0</kd>
</gazebo>

<gazebo reference="r_wheel">
