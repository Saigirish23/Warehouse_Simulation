  <kp>1000000.0</kp>
  <kd>100.0</kd>
</gazebo>

<gazebo reference="back_caster_wheel">
  <material>Gazebo/Blue</material>
  <mu1>0.2</mu1>
  <mu2>0.05</mu2>
  <minDepth>0.005</minDepth>
  <kp>1000000.0</kp>
  <kd>100.0</kd>
</gazebo>

<gazebo reference="ballast_weight">
  <material>Gazebo/DarkGrey</material>
</gazebo>

<gazebo reference="lidar_mount">
  <material>Gazebo/DarkGrey</material>
</gazebo>

<gazebo reference="camera_mount_link">
