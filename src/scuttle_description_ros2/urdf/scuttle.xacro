<?xml version="1.0" ?>
<robot name="scuttle" xmlns:xacro="http://www.ros.org/wiki/xacro">

<!-- Materials for URDF visualization -->
<material name="silver">
  <color rgba="0.700 0.700 0.700 1.000"/>
</material>

<material name="gray">
  <color rgba="0.250 0.250 0.250 1.000"/>
</material>

<material name="blue">
  <color rgba="0.000 0.000 0.700 1.000"/>
</material>

<material name="black">
  <color rgba="0.000 0.000 0.000 1.000"/>
</material>

<!-- Gazebo-specific materials using script references -->
<gazebo reference="base_link">
  <material>Gazebo/Grey</material>
  <mu1>0.2</mu1>
  <mu2>0.2</mu2>
  <gravity>true</gravity>
</gazebo>

<gazebo reference="chassis_link">
